{
  "name": "Chrome Leap Tools",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Chrome Leap Tools"
  },
  "manifest_version": 2
}
